    <Member MemberName="Location">
      <MemberSignature Language="C#" Value="public Microsoft.Office.Interop.Outlook.OlSelectionLocation Location { get; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype Microsoft.Office.Interop.Outlook.OlSelectionLocation Location" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Office.Interop.Outlook.Selection.Location" />
      <MemberSignature Language="VB.NET" Value="Public ReadOnly Property Location As OlSelectionLocation" />
      <MemberSignature Language="C++ CLI" Value="public:&#xA; property Microsoft::Office::Interop::Outlook::OlSelectionLocation Location { Microsoft::Office::Interop::Outlook::OlSelectionLocation get(); };" />
      <MemberType>Property</MemberType>
